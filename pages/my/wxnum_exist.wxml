<!--sign_result.wxml-->


<view >
   
<text>您的微信号已绑定学号</text>
    <scroll-view class="container">
        <view class="item">
            <label class="flex-view-item">已绑定学号</label>
            <text class="flex-view-item">{{num}}</text>
        </view>

        <view class="item">
            <label class="flex-view-item">已绑定学生</label>
            <text class="flex-view-item" style="font-weight:bold">{{name}}</text>
        </view>
     
    </scroll-view>
    
</view>
<button class="sure" bindtap="back">返回主页面</button>
